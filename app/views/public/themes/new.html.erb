<h1>Public::Themes#new</h1>

<div>
  <%= form_with model: @theme, local: true do |f| %>
    <label for="inputTitle", class="mb-2">題名</label>
    <%= f.text_field :title, id: "inputTitle", class: "form-control" %>
    <p class="mt-3 mb-2">詳細</p>
    <%= f.text_area :detail, class: "form-control" %>
    <%= f.submit '投稿する', class: "btn btn-success my-3" %>
  <% end %>
</div>
