<% themes.each do |theme| %>
  <div>
    <% if current_admin %>
      <%= link_to admin_theme_path(theme.id) do %>
        <%= image_tag theme.end_user.get_profile_image(50, 50) %>
        <%= theme.title %>
      <% end %>
    <% else %>
      <%= link_to theme_path(theme.id) do %>
        <%= image_tag theme.end_user.get_profile_image(50, 50) %>
        <%= theme.title %>
      <% end %>
    <% end %>
  </div>
<% end %>
